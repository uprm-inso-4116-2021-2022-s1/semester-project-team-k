<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - INSOProjectWebApplication</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-light bg-light" style="background: rgb(63,244,251); background: linear-gradient(90deg, rgba(63,244,251,1) 3%, rgba(84,187,235,1) 13%, rgba(127,109,216,1) 29%, rgba(120,87,208,1) 36%, rgba(165,85,214,1) 65%, rgba(252,70,248,1) 100%, rgba(246,71,112,1) 100%)";>
            <div class="container-fluid">
                <a class="navbar-brand" href="#">
                    <img src="~/Images/Logo.png" alt="" width="230" height="130" class="d-inline-block align-text-top">
                </a>
            </div>

            <div class="navbar-collapse collapse w-50 order-3 dual-collapse2">
                <ul class="navbar-nav ms-auto">
                    <li class="nav-item">
                        <a class="nav-link" style="color:white"  href="#">HOME</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color:white" href="#">ABOUT US</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" style="color:white" href="#">CONTACT US</a>
                    </li>
                </ul>
            </div>

        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
